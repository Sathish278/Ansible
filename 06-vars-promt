---
- name: vars from promt
  hosts: localhost
  vars_prompt:
  - name: Username
    prompt: please enter your username
    pravite: false
  - name: Password
    prompt: please enter your password
    pravite: true
  tasks:
  - name: printing information
    ansible.builtin.debug:
      msg: "your Username:{{Username}}. And Password:{{Password}}"
